# ── Copy to .env and fill in your values ──────────────────────────────────────

# Postgres connection string
DATABASE_URL=postgresql://postgres:yourpassword@localhost:5432/neurothrive

# JWT secret — generate with:  openssl rand -hex 32
SECRET_KEY=replace_me_with_a_long_random_secret

# Token lifetime in minutes (default: 1440 = 24 hours)
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Allowed frontend origins (comma-separated if using env parsing)
# FRONTEND_ORIGINS=["http://localhost:5173","http://localhost:3000"]
